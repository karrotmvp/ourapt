{
  "AWSEBDockerrunVersion": "1",
  "Image": {
    "Name": "$OURAPT_IMAGE_REPO:$OURAPT_IMAGE_TAG",
    "Update": "true"
  },
  "Ports": [
    {
      "ContainerPort": "$OURAPT_API_PORT"
    }
  ],
  "environment": [
    {
      "name": "RDB_HOST",
      "value": "$RDB_HOST"
    },
    {
      "name": "KARROT_APP_ID",
      "value": "$KARROT_APP_ID"
    },
    {
      "name": "KARROT_APP_SECRET",
      "value": "$KARROT_APP_SECRET"
    },
    {
      "name": "KARROT_BASE_URL",
      "value": "$KARROT_BASE_URL"
    },
    {
      "name": "KARROT_API_KEY",
      "value": "$KARROT_API_KEY"
    }
  ]
}
