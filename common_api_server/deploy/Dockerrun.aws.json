{
  "AWSEBDockerrunVersion": "1",
  "Image": {
    "Name": "#OURAPT_IMAGE_REPO/#OURAPT_IMAGE_NAME:#OURAPT_IMAGE_TAG",
    "Update": "true"
  },
  "Ports": [
    {
      "ContainerPort": "#API_LISTENING_PORT"
    }
  ],
  "environment": [
    {
      "name": "RDB_HOST",
      "value": "#RDB_HOST"
    },
    {
      "name": "RDB_USERNAME",
      "value": "#RDB_USERNAME"
    },
    {
      "name": "RDB_PASSWORD",
      "value": "#RDB_PASSWORD"
    },
    {
      "name": "KARROT_APP_ID",
      "value": "#KARROT_APP_ID"
    },
    {
      "name": "KARROT_APP_SECRET",
      "value": "#KARROT_APP_SECRET"
    },
    {
      "name": "KARROT_API_KEY",
      "value": "#KARROT_API_KEY"
    },
    {
      "name": "KARROT_OPEN_API_BASE_URL",
      "value": "#KARROT_OPEN_API_BASE_URL"
    },
    {
      "name": "KARROT_O_API_BASE_URL",
      "value": "#KARROT_O_API_BASE_URL"
    }
  ]
}
